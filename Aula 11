# Banco-de-Dados
Aulas



/*  select * from cursos
order by nome desc;  order by ordena a lista desc fica em ordem decrecente

*/
select nome, ano from cursos
order by ano, nome;

select * from cursos
where ano = '2016'
order by nome;

select nome, descricao,ano from cursos
where ano != '2015'   /* Operadores >, < , =>, <=, = , <> , 1+*/
order by nome;

select nome, ano from cursos
where ano between 2014 and 2016 /* faixa de valores*/
order by ano desc, nome; 

select nome, descricao, ano from cursos
where ano in (2014,2016)
order by ano;

select * from cursos
where carga > 35 and totaulas <30 /* and eo or*/
order by nome;
